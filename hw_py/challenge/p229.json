dictionary={
    "name":"기사",
    "level":13,
    "items":{
        "sword":"검",
        "armor":"어"
    },
    "skill":["베기","세게 베기","아주 세게 베기"]
}

for key in dictionary:
    if type(dictionary[])==str:
        print(key,":",dictionary[])
    elif type(dictionary[])==int:
        print(key,":",dictionary[])
    else:
        print(key,":",dictionary)